    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-citation": "^1.0.2",
    "rehype-katex": "^6.0.3",
    "rehype-preset-minify": "6.0.0",
    "rehype-prism-plus": "^1.6.0",
    "rehype-slug": "^6.0.0",
    "remark": "^15.0.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "tailwindcss": "^3.4.1",
    "unist-util-visit": "^5.0.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@types/mdx": "^2.0.10",
